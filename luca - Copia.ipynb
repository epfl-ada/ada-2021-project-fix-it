    "def removing_not_inherent_quotes(list_of_words, df, aliases):\n",
    "    df_copy = df\n",
    "    for word in list_of_words:\n",
    "        df_copy = df_copy[~df_copy['quotation'].str.contains(word)]\n",
    "    df_new = df_copy[~df_copy['speaker'].isin(aliases)]\n",
    "    return df_new"
   ]
